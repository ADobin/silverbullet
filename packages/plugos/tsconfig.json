{
  "include": ["bin/*", "environments/*", "hooks/*", "syscalls/*", "*"],
  "compilerOptions": {
    "target": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "module": "esnext",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "downlevelIteration": true
  }
}
